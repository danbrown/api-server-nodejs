{
  "name": "node-server",
  "version": "1.0.0",
  "author": "AppSeed",
  "license": "ISC",
  "type": "module",
  "scripts": {
    "start": "./scripts/develop.cjs ./source/server.js",
    "dev": "nodemon -X './scripts/develop.cjs ./source/server.js' --watch ./source",
    "lint": "npx eslint --ignore-path .gitignore .",
    "lint:fix": "npx eslint --ignore-path .gitignore --fix .",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|mjs|json)\"",
    "format": "npm run prettier -- --write && npx prisma format",
    "format:check": "npm run prettier -- --list-different",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "^2.13.1",
    "cors": "^2.8.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.0",
    "express-async-errors": "^3.1.0",
    "express-jwt": "^6.0.0",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.0",
    "lodash": "^4.17.20",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pino": "^6.9.0",
    "pino-pretty": "^4.3.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@esbuild-plugins/node-resolve": "^0.0.10",
    "@prisma/cli": "^2.13.1",
    "axios": "^0.21.1",
    "chance": "^1.1.7",
    "esbuild-jest": "^0.3.0",
    "esbuild-node": "^0.4.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.3.0",
    "husky": "^5.0.0",
    "jest-watch-typeahead": "^0.6.0",
    "lint-staged": "^10.5.0"
  },
  "peerDependencies": {
    "eslint": "^7.17.0",
    "jest": "^26.6.0",
    "nodemon": "^2.0.0",
    "prettier": "^2.2.0"
  }
}
